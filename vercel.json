{
    "rewrites":[
        {
            "source" :"/api/(.*)",
            "destination":"https://api.timbu.cloud/$1"
        },
        {
            "source" :"/(.*)",
            "destination":"/index.html"
        }
    ],
    "version": 2,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static-build",
        "config": {
          "framework": "vite"
        }
      }
    ]
}